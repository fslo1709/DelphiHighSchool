program mayor_dos_numeros_case;//Sebastián López
{$APPTYPE CONSOLE}
uses
  SysUtils;

var
  a,b: integer;
begin
  writeln ('Sebastián López');
  Writeln ('Ingrese dos números');
  Readln (a,b);
  case a>b of
    true: writeln('El mayor es a');
  else
    Writeln ('El mayor es b');
  end;
  Readln
end.
