program tres_algoritmos;//Sebastián López
{$APPTYPE CONSOLE}
uses
  SysUtils;

var
  a,b,c,d,x1,x2: real;
begin
  readln (a,b,c);
  d:= b*b-4*a*c;
  if d<0 then
    writeln ('No hay solución real')
  else
    begin
      x1:= (-b+sqrt(d))/(2*a);
      x2:= (-b-sqrt(d))/(2*a);
    end;
  readln
end.
